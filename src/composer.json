{
  "name": "better-tax-handling",
  "description": "Better Tax Handling is a plugin for WooCommerce stores that simplifies the complex part of taxation for B2B and B2C selling.",
  "keywords": [
    "WordPress",
    "plugin"
  ],
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "10up/wp_mock": "0.3.0",
    "antecedent/patchwork": "*",
    "php-coveralls/php-coveralls": "^2.1",
    "squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "edsonmedina/php_testability": "dev-master",
    "rregeer/phpunit-coverage-check": "^0.1.6",
    "phpunit/phpcov": "^5.0"
  },
  "minimum-stability": "stable",
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
    "psr-4": {
      "Niteo\\WooCart\\BetterTaxHandling\\": "framework/classes",
      "Niteo\\WooCart\\BetterTaxHandling\\Reports\\": "framework/classes/reports"
    }
  }
}
